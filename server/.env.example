# EKA-AI Platform Server Configuration
# Copy this file to .env and add your API keys

# Gemini API Key for AI features (if backend AI processing is needed)
GEMINI_API_KEY=your_gemini_api_key_here

# Server Configuration
PORT=5000

# Debug mode (set to 'true' only for development)
FLASK_DEBUG=false

# CORS Origins (comma-separated list, use '*' for all origins in development)
# For production, specify your domain(s): https://yourdomain.com,https://api.yourdomain.com
CORS_ORIGINS=*
